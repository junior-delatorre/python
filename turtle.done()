# turtle.done()
import turtle

t = turtle.Turtle()
for i in range(4):
	t.forward(50)
	t.right(90)     # Rotate clockwise by 90 degrees
for i in range(4):for i in range(4):
	t.forward(50)
	t.right(90)
for i in range(4):
	t.forward(50)
	t.right(90)
for i in range(4):
	t.forward(50)
	t.right(90)

	turtle.done()
